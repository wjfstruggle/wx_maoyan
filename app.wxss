.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
button::after{
  border:none;
  }
  input{
  outline:none;
  border:none;
  list-style: none;
  }
.textOverflow {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}